<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Admin</title>
    <!-- Google Font: Source Sans Pro -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
    <!-- Font Awesome Icons -->
    <link rel="stylesheet" href="~/Content/adminlte/plugins/fontawesome-free/css/all.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/Content/adminlte/dist/css/adminlte.min.css">
    
    @RenderSection("Styles", required: false)

    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/Data/css")
</head>
<body>

    <link href="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.min.css" rel="stylesheet" />


    <nav class="main-header navbar navbar-expand navbar-white navbar-light">
        <!-- Left navbar links -->
        <ul class="navbar-nav">
            <li class="nav-item">
                <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
            </li>
            <li class="nav-item d-none d-sm-inline-block">
                <a href="@Url.Action("Inicio","Admin")" class="nav-link">Inicio</a>
            </li>
        </ul>

        <!-- Right navbar links -->
        <ul class="navbar-nav ml-auto">

            <!-- Notifications Dropdown Menu -->

            <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" id="navbarDropdown" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false"><i class="fas fa-user fa-fw"></i></a>
                <ul class="dropdown-menu dropdown-menu-end" aria-labelledby="navbarDropdown">
                    <li><a class="dropdown-item" href="@Url.Action("InicioRegistro","Home")">Usuario</a></li>
                    <li><hr class="dropdown-divider" /></li>
                    <li><a class="dropdown-item" href="@Url.Action("Login","Acceso")" onclick="Function()">Cerrar Sesion</a></li>
                </ul>
            </li>


            <!-- Boton para pantalla completa -->
            <li class="nav-item">
                <a class="nav-link" data-widget="fullscreen" href="#" role="button">
                    <i class="fas fa-expand-arrows-alt"></i>
                </a>
            </li>
        </ul>
    </nav>

    @*
        Slidebar
    *@

    <aside class="main-sidebar sidebar-light-primary elevation-4">
        <!-- Brand Logo -->
        <a href="index3.html" class="brand-link">
            <img src="~/Content/Img/ML.png" alt="IMG" class="brand-image img-circle elevation-3" style="opacity: .8">
            <div class="navbar-brand ps-2"><font color="blue">SAFE</font></div>
        </a>

        <!-- Sidebar -->
        <div class="sidebar">
            <!-- Sidebar Menu -->
            <nav class="mt-2">
                <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">

                    <li class="nav-item">
                        <div class="navbar-brand ps-2" border="1"><font color="blue">Evento</font></div>
                        <a href="@Url.Action("Registro","Venta")" class="nav-link">

                            <i class="nav-icon fas fa-clipboard"></i>
                            <p>
                                Registro Venta
                            </p>
                        </a>
                    </li>

                    <li class="nav-item">
                        <div class="navbar-brand ps-2"><font color="blue">Control</font></div>

                        <a href="@Url.Action("Cliente","Admin")" class="nav-link">
                            <i class="nav-icon fas fa-book-open"></i>
                            <p>
                                Clientes
                            </p>
                        </a>


                    <li class="nav-item menu-open">

                        <a href="#" class="nav-link">
                            <i class="nav-icon fas fa-boxes"></i>
                            <p>
                                Servicios
                                <i class="right fas fa-angle-left"></i>
                            </p>
                        </a>
                        <ul class="nav nav-treeview">
                            <li class="nav-item">
                                <a href="@Url.Action("Almacen","Admin")" class="nav-link active">
                                    <i class="fas fa-warehouse nav-icon"></i>
                                    <p>Almacen</p>
                                </a>
                            </li>
                            <li class="nav-item">
                                <a href="@Url.Action("Transporte","Admin")" class="nav-link">
                                    <i class="fas fa-truck-moving nav-icon"></i>
                                    <p>Transporte</p>
                                </a>
                            </li>
                        </ul>
                    </li>

                    <li class="nav-item">

                        <div class="navbar-brand ps-2"><font color="blue">Facturacion</font></div>

                        <a href="@Url.Action("Historial","Admin")" class="nav-link">
                            <i class="nav-icon fas fa-book-open"></i>
                            <p>
                                Historial
                            </p>
                        </a>

                        <a href="@Url.Action("Proforma","Admin")" class="nav-link">
                            <i class="nav-icon fas fa-book-open"></i>
                            <p>
                                Cotizar
                            </p>
                        </a>
                    </li>

                    <li class="nav-item">
                        <div class="navbar-brand ps-2" border="1"><font color="blue">Empleados</font></div>
                        <a href="@Url.Action("Empleado","Admin")" class="nav-link">
                            <i class="nav-icon fas fa-book-open"></i>
                            <p>
                                Empleados
                            </p>
                        </a>

                        <a href="@Url.Action("Usuario","Admin")" class="nav-link">
                            <i class="nav-icon fas fa-book-open"></i>
                            <p>
                                Usuarios
                            </p>
                        </a>

                    </li>

                </ul>
            </nav>
            <!-- /.sidebar-menu -->
        </div>
        <!-- /.sidebar -->
    </aside>

    @RenderBody()

    @*
        Footer
    *@

    <footer class="main-footer">
        <!-- To the right -->
        <div class="float-right d-none d-sm-inline">
            Anything you want
        </div>
        <!-- Default to the left -->
        <strong>Copyright &copy; 2022 <a href="#">ML</a></strong>
    </footer>

    <script>
        function Modal() {
            $('#Modal').modal('show');
        }

        function enter1() {
            var x = document.getElementById("a");
            x.style.display = "none";
            var x = document.getElementById("b");
            x.style.display = "block";
        }

        function enter() {
            var x = document.getElementById("boton");
            x.style.display = "none";
            var x = document.getElementById("a");
            x.style.display = "block";
        }

    </script>



    <script src="https://code.jquery.com/jquery-3.5.1.js"></script>

    <script src="https://cdn.datatables.net/1.12.1/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/2.2.3/js/dataTables.buttons.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/2.2.3/js/buttons.html5.min.js"></script>

    <script src="https://cdn.datatables.net/buttons/2.2.3/js/buttons.print.min.js"></script>



    <!-- jQuery -->
    <script src="~/Content/adminlte/plugins/jquery/jquery.min.js"></script>
    <!-- Bootstrap 4 -->
    <script src="~/Content/adminlte/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/Content/adminlte/dist/js/adminlte.min.js"></script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.min.js"></script>


    <div class="modal fade" id="Modal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <center>
                        <h5 class="modal-title" id="exampleModalLabel">Cambiar Contraseña</h5>
                    </center>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-sm-12">
                            <input type="hidden" id="txtIdCliente" />
                            <div id="boton" class="form-group">
                                <center><label for="txtNombre">Ingrese contraseña actual:</label></center>
                                <input type="text" class="form-control" id="txtNombre">
                                <button class="form-control" onclick="enter()">Siguiente</button>
                            </div>
                            <div id="a" style="display:none" class="form-group">
                                <center><label for="txtNombre">Ingrese nueva contraseña:</label></center>
                                <input type="text" class="form-control" id="txt">
                                <button class="form-control" onclick="enter1()">Siguiente</button>
                            </div>
                            <div id="b" style="display:none" class="form-group">
                                <center> <label for="txtNombre">Ingrese nuevamente la contraseña:</label></center>
                                <input type="text" class="form-control" id="txtNombre">
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-lg" data-bs-dismiss="modal">Cerrar</button>
                    <button type="button" class="btn btn-primary" onclick="Guardar()">Guardar</button>
                </div>
            </div>
        </div>
    </div>



    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstra")
    @Scripts.Render("~/Content/Data/js")
    @Scripts.Render("~/bundles/complementos")

    @RenderSection("Scripts", required: false)

</body>
</html>

